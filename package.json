{
  "name": "@adobe/helix-coralogix-feeder",
  "version": "1.3.12",
  "private": true,
  "description": "Franklin Coralogix Feeder",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "test": "c8 mocha  -i -g 'Post-Deploy' --spec=test/**/*.test.js",
    "test-postdeploy": "mocha -g 'Post-Deploy' --spec=test/**/*.test.js",
    "lint": "eslint .",
    "logs": "aws logs tail /aws/lambda/spacecat-services--coralogix-feeder",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "build": "hedy -v",
    "deploy": "hedy -v --deploy --test --aws-region us-east-1 --params .env",
    "deploy-routes": "hedy --no-build --no-hints -l major",
    "deploy-ci": "hedy -v --deploy --test --pkgVersion=ci$CIRCLE_BUILD_NUM -l ci --cleanup-ci 24h",
    "prepare": "husky install"
  },
  "wsk": {
    "nodeVersion": 18,
    "target": "aws",
    "name": "spacecat-services/coralogix-feeder@${version}",
    "awsRole!important": "arn:aws:iam::282898975672:role/spacecat-role-coralogix-feeder",
    "testUrl": "/_status_check/healthcheck.json",
    "memory": 1024,
    "fastlyServiceId!important": ""
  },
  "mocha": {
    "require": "test/setup-env.js",
    "recursive": "true",
    "reporter": "mocha-multi-reporters",
    "reporter-options": "configFile=.mocha-multi.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-coralogix-feeder"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-coralogix-feeder/issues"
  },
  "homepage": "https://github.com/adobe/helix-coralogix-feeder#readme",
  "dependencies": {
    "@adobe/fetch": "4.1.1",
    "@adobe/helix-shared-wrap": "2.0.0",
    "@adobe/helix-status": "10.0.11",
    "aws4": "1.12.0"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "2.0.5",
    "@adobe/helix-deploy": "9.4.1",
    "@adobe/helix-universal": "4.4.1",
    "@adobe/semantic-release-coralogix": "1.1.27",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "c8": "8.0.1",
    "codecov": "3.8.3",
    "dotenv": "16.3.1",
    "eslint": "8.55.0",
    "husky": "8.0.3",
    "junit-report-builder": "3.1.0",
    "lint-staged": "15.2.0",
    "mocha": "10.2.0",
    "mocha-multi-reporters": "1.5.1",
    "nock": "13.4.0",
    "semantic-release": "22.0.10"
  },
  "lint-staged": {
    "*.js": "eslint"
  }
}
